{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": false,
    "declaration": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators":true,
     "importHelpers": true,
    "target": "es5",
    "skipLibCheck":true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "pl-core-utils-library": [
        "dist/pl-core-utils-library"
      ],
      "pl-core-utils-library/*": [
        "dist/pl-core-utils-library/*"
      ]
    }
  },
  "exclude": [
    "src/**/*.*",
  ]
}